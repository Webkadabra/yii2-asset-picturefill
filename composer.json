{
    "name": "webkadabra/yii2-picturefill-asset",
    "description": "Yii2 Picturefill plugin Asset",
    "type": "yii2-extension",
    "license": "MIT",
	"keywords": [
		"yii2",
		"picturefill",
		"html5"
	],
    "authors": [
        {
            "name": "Sergii Gamaiunov",
            "email": "i@sergiigama.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "bower-asset/picturefill": "3.0.*"
    },
    "autoload": {
        "psr-4": {
            "yii\\assets\\picturefill\\": ""
        }
    },
	"extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
